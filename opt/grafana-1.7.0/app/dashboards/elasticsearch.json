{
  "title": "ES Metrics",
  "tags": [],
  "style": "light",
  "timezone": "browser",
  "editable": true,
  "rows": [
    {
      "title": "Desc",
      "height": "75px",
      "editable": true,
      "collapse": false,
      "collapsable": true,
      "panels": [
        {
          "error": false,
          "span": 12,
          "editable": true,
          "type": "text",
          "loadingEditor": false,
          "mode": "markdown",
          "content": "This dashboard shows the various elasticsearch metrics.",
          "style": {},
          "title": "Elasticsearch Metrics"
        }
      ],
      "notice": false
    },
    {
      "title": "Compute Traffic",
      "height": "250px",
      "editable": true,
      "collapse": false,
      "collapsable": true,
      "panels": [
        {
          "span": 12,
          "editable": true,
          "type": "graph",
          "loadingEditor": false,
          "datasource": null,
          "renderer": "flot",
          "x-axis": true,
          "y-axis": true,
          "scale": 1,
          "y_formats": [
            "bytes",
            "short"
          ],
          "grid": {
            "max": null,
            "min": null,
            "threshold1": null,
            "threshold2": null,
            "threshold1Color": "rgba(216, 200, 27, 0.27)",
            "threshold2Color": "rgba(234, 112, 112, 0.22)",
            "leftMax": null,
            "rightMax": null,
            "leftMin": 0,
            "rightMin": 0
          },
          "annotate": {
            "enable": false
          },
          "resolution": 100,
          "lines": true,
          "fill": 0,
          "linewidth": 1,
          "points": false,
          "pointradius": 5,
          "bars": false,
          "stack": false,
          "legend": {
            "show": true,
            "values": true,
            "min": false,
            "max": false,
            "current": true,
            "total": false,
            "avg": false
          },
          "percentage": false,
          "zerofill": true,
          "nullPointMode": "connected",
          "steppedLine": false,
          "tooltip": {
            "value_type": "cumulative",
            "query_as_alias": true
          },
          "targets": [
            {
              "target": "aliasByNode(servers.compute*.network.eth0.tx_byte,1,4)",
              "function": "mean",
              "column": "value",
              "series": "servers.elk.elasticsearch.process.cpu.percent",
              "query": "select  mean(value) from \"servers.elk.elasticsearch.process.cpu.percent\" where  time > now() - 15m     group by time(1s)  order asc",
              "interval": "1s"
            },
            {
              "target": "aliasByNode(servers.compute*.network.eth0.tx_byte,1,4)",
              "function": "mean",
              "column": "value",
              "series": "servers.elk.elasticsearch.process.mem.resident",
              "query": "select  mean(value) from \"servers.elk.elasticsearch.process.mem.resident\" where  time > now() - 15m     group by time(1s)  order asc",
              "interval": "1s"
            }
          ],
          "aliasColors": {},
          "aliasYAxis": {
            "servers.elk.elasticsearch.process.mem.resident.mean": 1,
            "servers.elk.elasticsearch.process.cpu.percent.mean": 2
          },
          "title": "Elasticsearch Metrics"
        }
      ],
      "notice": false
    }
  ],
  "pulldowns": [
    {
      "type": "filtering",
      "collapse": false,
      "notice": false,
      "enable": false
    },
    {
      "type": "annotations",
      "enable": true,
      "annotations": [
        {
          "name": "syslog-ng",
          "datasource": "elasticsearch",
          "showLine": true,
          "iconColor": "#C0C6BE",
          "lineColor": "rgba(255, 96, 96, 0.592157)",
          "iconSize": 13,
          "enable": true,
          "index": "logstash-*",
          "query": "tags:syslog_out",
          "timeField": "@timestamp",
          "tagsField": "tags"
        }
      ]
    }
  ],
  "nav": [
    {
      "type": "timepicker",
      "collapse": false,
      "notice": false,
      "enable": true,
      "status": "Stable",
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ],
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "now": true
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "refresh": false,
  "version": 2
}